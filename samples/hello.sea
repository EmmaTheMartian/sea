raw[ // Includes :3
#include <stdio.h>
#include <stdbool.h>
]

/// Represents a given user :3
rec User(name: ^char, age: int)

fun greet(user: User): void {
	printf("Hello, %s!\n", user.name)
}

/**
 * Main function that does main things
 */
fun main(argc: int, argv: ^char[]): int {
	var users: User[] = [
		new User("Frodo", 50),
		new User("Gandalf", 50000),
	]

	each user of users {
		greet(user)
	}
}
